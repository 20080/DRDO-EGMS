<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.text.*?>
<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane prefHeight="146.0" prefWidth="318.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.CustomCryptoPrompt">
   <children>
      <Label layoutX="14.0" layoutY="6.0" text="Enteryour 16 digit SecretKey">
         <font>
            <Font name="Calibri" size="15.0" />
         </font>
      </Label>
      <Label layoutX="8.0" layoutY="126.0" text="If you think you are seeing this by mistake you must ReInstall ThisSoftware">
         <font>
            <Font name="Calibri" size="10.0" />
         </font>
      </Label>
      <PasswordField fx:id="Field" layoutX="14.0" layoutY="48.0" />
      <Button fx:id="Button" layoutX="111.0" layoutY="82.0" mnemonicParsing="false" onAction="#ButtonAction" text="Submit" />
      <CheckBox fx:id="Tickk" layoutX="179.0" layoutY="52.0" mnemonicParsing="false" onAction="#OnTickAction" text="Don't Ask Again" />
   </children>
</AnchorPane>
